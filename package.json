    "react-native-web": "~0.18.10",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.9",
    "@babel/node": "^7.22.6",
    "@babel/preset-env": "^7.22.9",
    "@flydotio/dockerfile": "^0.4.2",
    "nodemon": "^3.0.1"
  }
