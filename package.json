{
  "name": "loveoneanother_japanesewritinghelper",
  "version": "1.0.0",
  "description": "A webhook to connect to Facebook messenger",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --exec babel-node src/server.js",
